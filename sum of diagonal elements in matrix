#include <stdio.h>
int main()
{
    int m[10][10],r,c,i,j,sum=0;
    printf("enter the number of rows:");
    scanf("%d",& r);
    printf("enter the number of columns:");
    scanf("%d",& c);
    printf("enter the elements of matrix:");
    for(i=0;i<r;i++){
        for(j=0;j<c;j++){
            scanf("%d",& m[i][j]);
        }
    }
    for(i=0;i<r;i++){
        for(j=0;j<c;j++){
            printf("%d ",m[i][j]);
        }
        printf("\n");
    }
    for(i=0;i<r;i++){
        for(j=0;j<c;j++)
        if(i=j)
          sum=sum+m[i][j];
        }
          printf("sum of diagonal elements:%d",sum);
    return 0;
}
