# include <stdio.h>
int main()
{
    int min,max, i, arr[5];
    printf("enter the values:");
    for (i=0;i<5;i++){
      scanf("%d",& arr[i]);
    }
    max=arr[0];
    min=arr[0];
    for(i=0;i<5;i++)
    if (max<arr[i]){
       max=arr[i];}
       printf("maximum element:%d\n",max);
    if(min>arr[i]){
       min=arr[i];}
       printf("minimum element:%d",min);
    return 0;
      
}
